{
	"name": "bios-cat",
	"publisher": "CatMaster",
	"icon": "Images/01_CatIcon.png",
	"version": "0.0.1",
	"engines": { "vscode": "^1.59.0" },
	"displayName": "BIOS-CAT",
	"description": "A coding cat can support you development BIOS code!",
	"categories": ["Other"],
	"activationEvents": ["*"],
	"main": "./out/extension.js",
	"contributes": {
		"grammars": [
			{"language": "edk2_dsc", "scopeName": "source.edk2_dsc", "path": "./syntaxes/edk2_dsc.tmLanguage.json"},
			{"language": "edk2_dec", "scopeName": "source.edk2_dec", "path": "./syntaxes/edk2_dec.tmLanguage.json"},
			{"language": "edk2_fdf", "scopeName": "source.edk2_fdf", "path": "./syntaxes/edk2_fdf.tmLanguage.json"},
			{"language": "edk2_inf", "scopeName": "source.edk2_inf", "path": "./syntaxes/edk2_inf.tmLanguage.json"},
			{"language": "edk2_uni", "scopeName": "source.edk2_uni", "path": "./syntaxes/edk2_uni.tmLanguage.json"},
			{"language": "edk2_vfr", "scopeName": "source.edk2_vfr", "path": "./syntaxes/edk2_vfr.tmLanguage.json"},
			{"language": "edk2_asl", "scopeName": "source.edk2_asl", "path": "./syntaxes/edk2_asl.tmLanguage.json"}
		],
		"configuration": {
			"title": "BIOS-CAT ~",
			"properties": {
				"CAT.00_BuildPath": {
					"type": "string",
					"default" : "Intel/AlderLakeBoardPkg",
					"description": "This field is for your command bat path, you can gave two way : \n  1. Absolute path. ex: c://User//Workspace/CommandFolder \n  2. Relative path. ex: Workspace/CommandFolder"
				},
				"CAT.01_PreBuildCmd": {
					"type": "string",
					"default" : "prep",
					"description": "Here is for pre build command or bat file that you can establish your environment."
				},
				"CAT.02_BuildCmd": {
					"type": "string",
					"default" : "bld",
					"description": "Here is for build command or bat file that you can build your code  ex: nmake XXX or Build.bat ...."
				},
				"CAT.Parameter": {
					"type": "object",
					"default": {
						"Debug": "",
						"Release": "r",
						"FSP-API Debug": "fspapi",
						"FSP-API Release": "fspapi r"
					},
					"additionalProperties": {
						"type": "string"
					},
					"description": "Build command Parameter setting 01 ex. efidebug or perf .etc"
				},
				"CAT.03_CleanCmd": {
					"type": "string",
					"default" : "cln",
					"description": "The command that can clean your workspace."
				},
				"CAT.04_SetParameterWith": {
					"type": "string",
					"default": "PreBuild",
					"enum": ["PreBuild", "Build"],
					"enumDescriptions": [
					  "The prameter will go with [PreBuild] command.",
					  "The prameter will go with [Build] command."
					]
				},
				"X.ShowTip": {
					"type": "boolean",
					"default": true,
					"description": "Show this page next time ??"
				}
			}
		},
		"commands": [
			{"command": "BIOS-CAT.CMD01"            , "title": "01_Build code"},
			{"command": "BIOS-CAT.CMD02"            , "title": "02_Clean"},
			{"command": "BIOS-CAT.CMD03"            , "title": "03_Jump to Build log / error"},
			{"command": "BIOS-CAT.CMD04"            , "title": "04_BuildThisModule (With Makefile)"},
			{"command": "BIOS-CAT.L01AddMark"       , "title": "Add BookMark ðŸ“’"    , "icon": "./Images/L01_Add.png"},
			{"command": "BIOS-CAT.L01Reflash"       , "title": "Reflash BookMark ðŸ“’", "icon": "./Images/L01_Reflash.png"},
			{"command": "BIOS-CAT.L01Edit"          , "title": "Edit BookMark ðŸ“’"   , "icon": "./Images/L01_Edit.png"},
			{"command": "BIOS-CAT.L01Delete"        , "title": "Delete BookMark ðŸ“’" , "icon": "./Images/L01_Delete.png"},
			{"command": "BIOS-CAT.L01JumpToFile"    , "title": "Jump to file ðŸ“’"    , "icon": "./Images/L01_Jump.png"},
			{"command": "BIOS-CAT.L01CopyFullPath"  , "title": "Copy Full Path"},
			{"command": "BIOS-CAT.L01CopyFolderPath", "title": "Copy Folder Path"},
			{"command": "BIOS-CAT.L01CopyFileName"  , "title": "Copy File Name"},
			{"command": "BIOS-CAT.L02StartRecord"   , "title": "Start/Stop record log", "icon": "./Images/L01_Jump.png"}
		],
		"keybindings": [
			{"command": "BIOS-CAT.CMD01",            "key": "alt+'",   "when": "editorTextFocus"},
			{"command": "BIOS-CAT.CMD02",            "key": "alt+;",   "when": "editorTextFocus"},
			{"command": "BIOS-CAT.CMD03",            "key": "alt+/",   "when": "editorTextFocus"},
			{"command": "BIOS-CAT.CMD04",            "key": "alt+.",   "when": "editorTextFocus"},
			{"command": "BIOS-CAT.L01AddMark",       "key": "ctrl+m",  "when": "editorTextFocus", "mac": "cmd+m"},
			{"command": "BIOS-CAT.L01CopyFullPath",  "key": "alt+p",   "when": "editorTextFocus", "mac": "cmd+p"},
			{"command": "BIOS-CAT.L01CopyFolderPath","key": "alt+[",   "when": "editorTextFocus"},
			{"command": "BIOS-CAT.L01CopyFileName",  "key": "alt+]",   "when": "editorTextFocus"},
			{"command": "BIOS-CAT.L02StartRecord",   "key": "alt+r",   "when": "editorTextFocus"}
		],
		"menus": {
			"view/title": [
				{ "command": "BIOS-CAT.L01AddMark", "when": "view == L01", "group": "navigation" },
				{ "command": "BIOS-CAT.L01Reflash", "when": "view == L01", "group": "navigation" }
			],
			"view/item/context": [
				{ "command": "BIOS-CAT.L01Edit", "when": "view == L01", "group": "inline" },
				{ "command": "BIOS-CAT.L01Delete", "when": "view == L01", "group": "inline" },
				{ "command": "BIOS-CAT.L01JumpToFile", "when": "view == L01  && viewItem == Depn_M", "group": "inline" }
			],
			"editor/context": [
				{ "when": "editorFocus", "command": "BIOS-CAT.CMD01", "group": "navigation" },
				{ "when": "editorFocus", "command": "BIOS-CAT.CMD02", "group": "navigation" },
				{ "when": "editorFocus", "command": "BIOS-CAT.CMD03", "group": "navigation" },
				{ "when": "editorFocus", "command": "BIOS-CAT.CMD04", "group": "navigation" },
				{ "when": "editorFocus", "command": "BIOS-CAT.L01AddMark", "group": "navigation" }
			]
		},
		"viewsContainers": {"activitybar": [{ "id": "CatMaster", "title": "BIOS-CAT", "icon": "./Images/00_CatIcon.png" }]},
		"views": {
			"CatMaster": [
				{"id": "L01", "name": "ã€€ðŸ“’ Bookmark ~"},
				{"id": "L02", "name": "ã€€ðŸ’» Record Log"},
				{"id": "L03", "name": "ã€€View port 03"}
			]
		},
		"languages": [
			{"id": "edk2_dsc", "extensions": [".dsc"], "configuration": "./confs/edk2_dsc.conf.json"},
			{"id": "edk2_dec", "extensions": [".dec"], "configuration": "./confs/edk2_dec.conf.json"},
			{"id": "edk2_fdf", "extensions": [".fdf"], "configuration": "./confs/edk2_fdf.conf.json"},
			{"id": "edk2_inf", "extensions": [".inf"], "configuration": "./confs/edk2_inf.conf.json","aliases": ["Inf", "inf"]},
			{"id": "edk2_uni", "extensions": [".uni"], "configuration": "./confs/edk2_uni.conf.json"},
			{"id": "edk2_vfr", "extensions": [".vfr",".Vfr",".hfr",".Hfr"], "configuration": "./confs/edk2_vfr.conf.json"},
			{"id": "edk2_asl", "extensions": [".asl",".asi",".ASL",".ASI"], "configuration": "./confs/edk2_asl.conf.json"}
		],
		"snippets": [
			{"language": "c", "path": "./Snippets/C.json"},
			{"language": "edk2_inf", "path": "./Snippets/Inf.json"}
		]
	},
	"repository": {
		"type": "git",
		"url": "https://gitlab.com/CatMasterDB/bios-cat.git"
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.59.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"eslint": "^7.27.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"typescript": "^4.3.2",
		"vscode-test": "^1.5.2"
	}
}
